{
  "name": "tree-sitter-ferret",
  "version": "0.0.1",
  "description": "Tree-sitter grammar for the Ferret programming language",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "ferret",
    "syntax"
  ],
  "author": "Fuad",
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "optionalDependencies": {
    "nan": "^2.17.0"
  },
  "tree-sitter": [
    {
      "scope": "source.ferret",
      "file-types": [
        "fer"
      ],
      "injection-regex": "^ferret$",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itsfuad/tree-sitter-ferret.git"
  }
}
