{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ferret",
      "camelcase": "Ferret",
      "title": "Ferret",
      "scope": "source.ferret",
      "file-types": [
        "ferret",
        "fer"
      ],
      "injection-regex": "^ferret$",
      "class-name": "TreeSitterFerret"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Ferret Language Support",
    "authors": [
      {
        "name": "Fuad Hasan",
        "email": "fuad.cs22@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/itsfuad/tree-sitter-ferret"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}